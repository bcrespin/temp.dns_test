                                {{ bind_zones[item].refresh|default(bind_zone_default_refresh) }} ; refresh
                                {{ bind_zones[item].retry|default(bind_zone_default_retry) }} ; retry
                                {{ bind_zones[item].expire|default(bind_zone_default_expire) }} ; expire
                                {{ bind_zones[item].nxdomain_ttl|default(bind_zone_default_nxdomain_ttl) }} ; nxdomain ttl
                             )
                NS      {{ bind_dns_fqdn_name }}.

