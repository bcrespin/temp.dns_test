$TTL {{ bind_zone_default_ttl }}
{% set serial = ansible_dns_file_zone_serial_number[bind_directory +'/' + bind_config_master_zones_path +'/'+ bind_zones[item].name +'.db']  %}
@		SOA	 {{ bind_dns_fqdn_name }}. {{ bind_hostmaster_name }}. (
